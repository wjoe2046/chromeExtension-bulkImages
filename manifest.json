{
  "name": "ImgCrawl",
  "description": "Crawl images on a webpage and download it to your local drive",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
